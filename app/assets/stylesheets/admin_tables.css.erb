.recordnumber{
	font-size:14px;
	font-weight:normal!important; 
	text-transform:uppercase;
	margin-bottom:10px; 
}

table.listing{
	margin-top:10px;
	border:1px solid #000000;
	width:950px;
}

table.listing th{
	border:1px solid #ffffff;
	padding:10px;
	height:50px;
	background-color:#000000;
	color:#ffffff;
	text-transform:uppercase;
	font-size:16px;
	font-weight:normal!important;
	text-align:left;
	border:1px solid #ffffff;  
}

table.listing td{
	font-size:14px;
	font-weight:normal!important;  
	border:1px solid #000000;
	padding:10px;
}

.odd{
	background-color:#dddddd; 
}

.even{
	background-color:#aaaaaa; 
}
