#pagetitlecontainer{
	position:relative;
	width:100%;
	height:50px;
	background-color:#deddd3;
	border-top:1px solid #848484;
	border-bottom:1px solid #848484;
}

#pagetitlecontent{
	position:relative;
	width:970px;
	height:50px;
	margin-right:auto;
	margin-left:auto;  
	background-color:#deddd3;
}

#pagetitlecontent h3{
	font-size:20px;
	font-weight:normal!important;
	text-align:center;  
	padding-top:12px;
	text-transform:uppercase; 
}

#noticecontainer{
	position:relative;
	width:100%;
	height:25px;
	background-color:#b2e98d;
	border-top:1px solid #326014;
	border-bottom:1px solid #326014;
}

#noticecontent{
	position:relative;
	width:970px;
	height:25px;
	margin-right:auto;
	margin-left:auto;  
	background-color:#b2e98d;
}

#noticecontent h4{
	font-size:12px;
	font-weight:normal!important;
	text-align:center;  
	padding-top:5px;
	color:#326014;
}

#errornoticecontainer{
	position:relative;
	width:100%;
	height:25px;
	background-color:#d24444;
	border-top:1px solid #ab0702;
	border-bottom:1px solid #ab0702;
}

#errornoticecontent{
	position:relative;
	width:970px;
	height:25px;
	margin-right:auto;
	margin-left:auto;  
	background-color:#d24444;
}

#errornoticecontent h4{
	font-size:12px;
	font-weight:normal!important;
	text-align:center;  
	padding-top:5px;
	color:#ffffff;
}

