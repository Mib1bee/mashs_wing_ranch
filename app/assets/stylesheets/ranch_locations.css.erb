html body #mapscontainer{height:100%;}

#mapscontainer{
	position:relative;
	top:0px;
	left:0px;
	min-height:1000px;
	min-width:1280px;
	max-width:100%;
	margin-top:63px;
	overflow:hidden; 
}

#ranchcontainer{
	position:absolute;
	top:0px;
	left:10px;
}

#ranchlist{
position:absolute;
top:0px;
left:210px;
width:300px;
min-height:650px;
background-image:url(locations/locations_list_bg.png);
background-repeat:repeat;
padding-top:40px;   
padding-left:20px; 
padding-bottom:50px;
}

#ranchlist h1{
	font-family:'Bebas'; 
	font-size:90px;
	font-weight:normal!important;
	color:#f0e7cd; 
	line-height:70px; 
}

#ranchlist h2{
	font-family:'Bebas'; 
	font-size:30px;
	font-weight:normal!important;  
	color:#d43800;
}

#ranchlist li{
	margin-top:25px;
}

.ranch_name{
	font-family:'Bebas'; 
	color:#f0e7cd;
	font-size:26px;
	text-transform:uppercase;
	text-decoration:underline; 
	font-weight:normal!important;   
}

.ranch_name:hover{
	color:#d43800;
}

.ranch_tel{
	font-family:'Signika'; 
	color:#d43800;
	font-size:16px;
	font-weight:normal!important;
}

.ranch_address{
	font-family:'Signika'; 
	color:#f0e7cd;
	font-size:18px;
	font-weight:normal!important;
	line-height:18px; 
}

.lineseperator{
	width:275px;
}

.listseperator{
	width:275px;
	margin-top:25px;
}

.ranch_info{
	font-family:'Signika';
	font-size: 16px;
	font-weight:normal!important;  
}

.info_tel{
	font-family:'Signika';
	font-size: 14px;
	font-weight:normal!important;
	margin-top:5px;
	color:#d43800;
}
