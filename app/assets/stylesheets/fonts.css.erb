/* Font face css for Qlassik */
@font-face {
    font-family: 'Qlassik';
    src: url('<%= asset_path('qlassik/qlassik.eot') %>');
    src: url('<%= asset_path('qlassik/qlassik.eot?#iefix') %>') format('embedded-opentype'),
         url('<%= asset_path('qlassik/qlassik.woff') %>') format('woff'),
         url('<%= asset_path('qlassik/qlassik.ttf') %>') format('truetype'),
         url('<%= asset_path('qlassik/qlassik.svg#Qlassik') %>') format('svg');
    font-weight:normal;
    font-style:normal;  
}

