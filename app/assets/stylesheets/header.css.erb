/* HEADER SECTION */

/* HEADER CONTAINER */
#headercontainer{
	position:fixed;
	background-color:#1a1813; 
	top:0px;
	left:0px;
	width:100%;
	height:63px;
	margin-top:0;
	margin-left:0;
	z-index:100;
	}
/* END HEADER CONTAINER */

/* HEADER CONTENT */
#headercontent{
	position:relative;
	background-image:no-repeat; 
	top:0px;
	left:0px;
	width:1280px;
	margin-left:auto;
	margin-right:auto;
}
/* END HEADER CONTENT */

/* HEADER BACKGROUND */
#headerbg{
	background-image:url(header/header_bg.png);
	background-repeat:no-repeat; 
	margin-left:auto;
	margin-right:auto;
	width:1280px;
	height:103px;

}
/* END HEADER BACKGROUND */

/* HEADER LOGO */
#headerlogo{
	background-image:url(header/logo.png); 
	background-repeat:no-repeat;
	width:150px;
	height:85px;
	position:absolute;
	top:0px;
	left:206px;
	cursor: pointer;
}
/* END HEADER LOGO */

/* HEADER NAVIGATION */
#headernavigation{
	position:absolute;
	top:25px;
	left:370px;
	margin:0;
	padding:0;
	font-size:14px;
	font-weight:200;
	color:#f03738;
	z-index:2;
}

/* NAVIGATION LIST */
#headernavigation li{
	display:inline;
	list-style-type:none;
	text-transform:uppercase; 
	margin-right:25px;
}
/* END NAVIGATION LIST HOVER */

/* NAVIGATION LIST */
#headernavigation li:hover{
	position:relative;
	top:1px;
	color:#fdb735;
	
}

#headernavigation a{
	cursor: pointer;
	color:#f03738;
}


/* END NAVIGATION LIST HOVER*/

/* END HEADER NAVIGATION */

/* HEADER RANCH REWARDS CARD */
#headercard{	
	background-image:url(header/card.png);
	background-repeat:no-repeat;
	width:104px;
	height:59px;
	position:absolute;
	top:0px;
	left:836px;
	cursor:pointer;
	/* CSS TRANSITION EFFECTS */
	-webkit-transition: background-image 0.25s linear;
	-moz-transition: background-image 0.25s linear;
	-o-transition: background-image 0.25s linear;
	transition: background-image 0.25s linear;
}

/* RANCH REWARDS CARD LINK HOVER */
#headercard:hover{
	position:relative;
	top:1px;
	background-image:url(header/card_hover.png);
}
/* END RANCH REWARDS CARD LINK HOVER */
/* END HEADER RANCH REWARDS CARD */

/* HEADER SOCIAL NETWORK LINKS */
#headersociallinks{
	background-image:url(header/social_links.png);
	background-repeat:no-repeat;
	width:75px;
	height:37px;
	position:absolute;
	top:10px;
	left:980px;
	cursor:pointer; 
	/* CSS TRANSITION EFFECTS */
	-webkit-transition: background-image 0.25s linear;
	-moz-transition: background-image 0.25s linear;
	-o-transition: background-image 0.25s linear;
	transition: background-image 0.25s linear;
}
/* HEADER SOCIAL NETWORK LINKS HOVER */
#headersociallinks:hover{
	position:relative;
	top:11px;
	background-image:url(header/social_links_hover.png);
}
/* END HEADER SOCIAL NETWORK LINKS HOVER */
/* END HEADER SOCIAL NETWORK LINKS */

/* HEADER LINKS */
/* END HEADER LINKS STYLE */
/* END HEADER SECTION */